{
	"name": "time-convert",
	"displayName": "time-convert",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "onCommand:time-convert.helloWorld",
    "onCommand:time-convert.getCurFilePath",
    "onCommand:time-convert.testEditorCommand",
    "onLanguage:json",
    "onCommand:time-convert.openWebview",
    "onCommand:time-convert.timeReadable"
	],
  "main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
        "command": "time-convert.helloWorld",
        "title": "hello World"
      },
      {
        "command": "time-convert.getCurFilePath",
        "title": "current path"
      },
      {
        "command": "time-convert.testEditorCommand",
        "title": "test editor"
      },
      {
        "command": "time-convert.openWebview",
        "title": "time-convert.openWebview"
      },
      {
        "command": "time-convert.timeReadable",
        "title": "time-convert.timeReadable"
      }
    ],
    "keybindings": [
      {
        "command": "time-convert.helloWorld",
        "key":"ctrl+f10",
        "mac":"cmd+f10",
        "when":"editorTextFocus"
      },
      {
        "command": "time-convert.testEditorCommand",
        "key":"ctrl+f11",
        "mac":"cmd+f11"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "time-convert.helloWorld",
          "group": "navigation"
        },
        {
           "when": "editorFocus",
          "command": "time-convert.getCurFilePath",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "time-convert.getCurFilePath",
          "group": "navigation"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
  }
}
